property newServerConnectRef1 ${newServerAcceptRef1} # external scope
property newServerConnectRef2 ${newServerAcceptRef2} # external scope

property serverConnect "nukleus://target/streams/example#source"
       
accept ${serverConnect}
       option nukleus:route ${newServerConnectRef1}
       option nukleus:window 8192
       option nukleus:transmission "duplex"
       option nukleus:authorization ${streamAuthorization1}
accepted
connected

accept ${serverConnect}
       option nukleus:route ${newServerConnectRef2}
       option nukleus:window 8192
       option nukleus:transmission "duplex"
       option nukleus:authorization ${streamAuthorization2}
accepted

connected

